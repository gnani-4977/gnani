def white(s1,s2):
    count=0
    for i in s1:
        if i==" ":
            count+=1
    count1=0        
    for j in s2:
        if j==" ":
            count1+=1
    sum=count-count1
    if sum%2==0:
        return f"even {sum}"
    else:
        return f"odd {sum}"
        
print(white("he ll owo r ld","he lo")) 
